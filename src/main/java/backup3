import java.io.*;

public class Main {

    // https://www.acmicpc.net/problem/10770

    private static char[] chars = {'a', 'e', 'i', 'o', 'u'};

    private static boolean isVowel(char c) {
        for (char ch : chars) {
            if (ch == c) {
                return true;
            }
        }

        return false;
    }

    private static char getNextConsonant(char c) {
        if (c == 'z') return 'z';

        char ret = (char) (c + 1);
        while (isVowel(ret)) {
            ret = (char) (ret + 1);
        }

        return ret;
    }

    private static char getClosestVowel(char c) {
        for (int i = 1; i < 'z' - 'a'; i++) {
            if (c >= 'a' && isVowel((char) (c - i))) {
                return (char) (c - i);
            }

            if (c  <= 'z' && isVowel((char) (c + i)) ) {
                return (char) (c + i);
            }
        }

        throw new IllegalStateException();
    }

    public static void problemSolver() throws IOException {
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
        BufferedWriter bw = new BufferedWriter(new OutputStreamWriter(System.out));

        char[] s = br.readLine().toCharArray();

        StringBuilder sb = new StringBuilder();

        for (char c : s) {
            sb.append(c);

            if (isVowel(c)) {
                continue;
            }

            sb.append(getClosestVowel(c));
            sb.append(getNextConsonant(c));
        }

        bw.write(sb.toString());

        bw.newLine();
        bw.close();
        br.close();
    }

    public static void main(String[] args) throws IOException {
        problemSolver();
    }
}